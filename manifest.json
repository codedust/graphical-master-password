{

  "name": "Graphical Master Password",
  "description": "Adds a shoulder-surfing resistant graphical master password to the firefox password manager",
  "manifest_version": 2,
  "version": "0.1",
  "homepage_url": "https://www.secuso.informatik.tu-darmstadt.de/en/secuso/",
  "icons": {
    "48": "icons/icon-48.png"
  },

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "icons/icon-32.png",
    "default_title": "Graphical Master Password",
    "default_popup": "popup/popup.html",
    "browser_style": false
  },

  "web_accessible_resources": [
  ]
}
